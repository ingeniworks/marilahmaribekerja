<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="GoWorkTwo.AboutPage">
    <ContentPage.Content>
        <StackLayout Padding="10,10,10,10">
            <StackLayout Orientation="Horizontal">
                <Image x:Name="iAPIcon" Margin="5,5,5,5"/>
                <StackLayout Orientation="Vertical" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                    <Label x:Name="lAPAppTitle" Text="Khije" FontSize="Large" />
                    <Label x:Name="lAPAppCopy" Text="Copyright (c) 2018 Ingeniworks"/>
                </StackLayout>
            </StackLayout>
            <StackLayout>
                <Label x:Name="lAPAppVersion" Text="Versi 1.0" />
                <Label x:Name="lAPAppBuildNo" Text="Nombor Binaan jenkins1" />
                <Label x:Name="lAPAppSysVer" Text="IOS" />
            </StackLayout>
            <StackLayout>
                <Label x:Name="lAPAppInfo" Text="Versi 1.0" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
